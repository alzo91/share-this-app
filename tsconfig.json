{
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": true,
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@navigations/*": ["src/navigations/*"],
      "@screens/*": ["src/screens/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"]
    },
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "types": ["jest", "react-native", "node", "styled-components-react-native"]
  },
  "extends": "expo/tsconfig.base"
}
